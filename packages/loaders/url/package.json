{
  "name": "@graphql-tools/url-loader",
  "version": "7.16.26",
  "description": "A set of utils for faster development of GraphQL tools",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-tools",
    "directory": "packages/loaders/url"
  },
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@envelop/live-query": "5.0.4",
    "@types/express": "4.17.14",
    "@types/extract-files": "8.1.1",
    "@types/valid-url": "1.0.3",
    "babel-loader": "9.1.0",
    "express": "4.18.2",
    "graphql-upload": "16.0.2",
    "graphql-sse": "1.3.2",
    "graphql-yoga": "3.1.1",
    "@graphql-yoga/plugin-defer-stream": "1.1.1",
    "puppeteer": "19.4.0",
    "subscriptions-transport-ws": "0.11.0",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@graphql-tools/delegate": "9.0.20",
    "@graphql-tools/executor-http": "0.0.7",
    "@graphql-tools/executor-graphql-ws": "0.0.5",
    "@graphql-tools/executor-legacy-ws": "0.0.5",
    "@graphql-tools/utils": "9.1.3",
    "@graphql-tools/wrap": "9.2.21",
    "@ardatan/sync-fetch": "0.0.1",
    "@whatwg-node/fetch": "^0.5.0",
    "@types/ws": "^8.0.0",
    "ws": "8.11.0",
    "isomorphic-ws": "5.0.0",
    "tslib": "^2.4.0",
    "value-or-promise": "^1.0.11"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
