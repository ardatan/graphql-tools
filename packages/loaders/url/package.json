{
  "name": "@graphql-tools/url-loader",
  "version": "7.2.0",
  "description": "A set of utils for faster development of GraphQL tools",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-tools",
    "directory": "packages/loaders/url"
  },
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "buildOptions": {
    "external": [
      "eventsource/lib/eventsource-polyfill"
    ]
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/valid-url": "1.0.3",
    "@types/extract-files": "8.1.1",
    "graphql-upload": "12.0.0",
    "mock-http": "1.1.0",
    "mock-socket": "9.0.3"
  },
  "dependencies": {
    "@graphql-tools/delegate": "^8.2.0",
    "@graphql-tools/utils": "^8.2.0",
    "@graphql-tools/wrap": "^8.1.0",
    "@ardatan/fetch-event-source": "2.0.2",
    "@n1ru4l/graphql-live-query": "0.8.1",
    "@types/websocket": "1.0.4",
    "@types/ws": "^7.4.7",
    "abort-controller": "3.0.0",
    "cross-fetch": "3.1.4",
    "extract-files": "11.0.0",
    "form-data": "4.0.0",
    "graphql-sse": "^1.0.1",
    "graphql-ws": "^5.4.1",
    "lodash": "4.17.21",
    "meros": "1.1.4",
    "is-promise": "4.0.0",
    "isomorphic-ws": "4.0.1",
    "subscriptions-transport-ws": "^0.10.0",
    "sync-fetch": "0.3.0",
    "tslib": "~2.3.0",
    "valid-url": "1.0.9",
    "ws": "8.2.2",
    "value-or-promise": "1.0.10"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
