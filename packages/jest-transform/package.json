{
  "name": "@graphql-tools/jest-transform",
  "version": "1.2.1",
  "description": "Jest Plugin to load and parse imported GraphQL files",
  "repository": {
    "type": "git",
    "url": "git@github.com:ardatan/graphql-tools.git",
    "directory": "packages/jest-transform"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "dependencies": {
    "@graphql-tools/webpack-loader": "6.7.1",
    "@graphql-tools/graphql": "*",
    "@jest/transform": "^28.0.0",
    "@jest/types": "^28.0.0",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
